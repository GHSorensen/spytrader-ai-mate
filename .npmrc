
# Ensure npm can resolve packages correctly
# Don't try to use Python
python=false
python-enabled=false
# Force npm to use Node.js
engine-strict=true
# Ensure we're using the right Node version
node-version=18.19.1
# Set registry to npm
registry=https://registry.npmjs.org/
# Explicitly disable any Python integration
ignore-python=true
python-disabled=true
